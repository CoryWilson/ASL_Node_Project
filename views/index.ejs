<!DOCTYPE html>
<html>
  <head>
   <% include head.ejs %>
  </head>
  <body class='<%=classname %>'>
  <% include header.ejs %>


  <div id="row1" class="row">

        <h1><%= title %></h1>

          <input placeholder="Search Mountains" name="searchbox" type="text" size="40" />

  </div>

  <div id="row2" class="row">
      <h1>Sign up and get started</h1>

      <form action="processingForm" method="post">

              <div class="small-10">
                  <div class="row">
                      <div class="small-3 columns">
                          <label for="right-label" class="right">Username</label>
                      </div>
                      <div class="small-9 columns">
                          <input type="text" id="right-label" placeholder="Username">
                      </div>

                      <div class="small-3 columns">
                          <label for="right-label" class="right">Email</label>
                      </div>

                      <div class="small-9 columns">
                          <input type="text" id="right-label" placeholder="Email">
                      </div>

                      <div class="small-3 columns">
                          <label for="right-label" class="right">Password</label>
                      </div>

                      <div class="small-9 columns">
                          <input type="password" id="right-label" placeholder="Password">
                      </div>

                      <div class="small-9 columns">
                        <a role="button" aria-label="submit form" href="#" class="button">Submit</a>
                      </div>

                  </div>
              </div>

      </form>

  </div>



  </body>
</html>
